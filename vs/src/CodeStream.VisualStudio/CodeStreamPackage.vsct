    <KeyBinding guid="guidWebViewPackageCodeWindowContextMenuCmdSet" id="AddCodemarkPermalinkCommandId" 
                editor="GUID_TextEditorFactory" mod1="Control Shift" key1="VK_OEM_2" key2="p" />
    <KeyBinding guid="guidWebViewPackageCodeWindowContextMenuCmdSet" id="AddCodemarkPermalinkInstantCommandId" 
                editor="GUID_TextEditorFactory" mod1="Control Shift" key1="VK_OEM_2" mod2="Shift" key2="p" />

    <KeyBinding guid="guidWebViewPackageCmdSet" id="WebViewDevToolsCommandId"
                editor="guidVSStd97" key1="C" mod1="ALT" key2="D" mod2="ALT" />
  </KeyBindings>

