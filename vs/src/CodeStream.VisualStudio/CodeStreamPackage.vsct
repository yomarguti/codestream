        <CommandFlag>DefaultInvisible</CommandFlag>
        <Strings>
          <ButtonText>Create Bookmark</ButtonText>
          <CanonicalName>CodeStream.CreateBookmark</CanonicalName>
          <LocCanonicalName>CodeStream.CreateBookmark</LocCanonicalName>
