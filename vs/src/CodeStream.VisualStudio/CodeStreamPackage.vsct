  <KeyBindings>
    <KeyBinding guid="guidWebViewPackageCmdSet" id="WebViewToggleCommandId"
                editor="guidVSStd97" key1="C" mod1="ALT" key2="S" mod2="ALT" />

    <KeyBinding guid="guidWebViewPackageCodeWindowContextMenuCmdSet" id="AddCodemarkCommentCommandId" 
                editor="GUID_TextEditorFactory" mod1="Control Shift" key1="VK_OEM_2" mod2="Control Shift" key2="VK_OEM_2" />
    <KeyBinding guid="guidWebViewPackageCodeWindowContextMenuCmdSet" id="AddCodemarkCommentCommandId" 
                editor="GUID_TextEditorFactory" mod1="Control Shift" key1="VK_OEM_2" key2="c" />
    <KeyBinding guid="guidWebViewPackageCodeWindowContextMenuCmdSet" id="AddCodemarkIssueCommandId" 
